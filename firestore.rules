rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /blogs/{document=**} {
      allow read;
      allow write: if
       request.auth != null && request.auth.uid == 'n3rqpItdiNe9puoNUAeNrZRo1Dx1'
    }
    match /comments/{document=**} {
      allow read, create;
      allow delete: if
       request.auth != null && request.auth.uid == 'n3rqpItdiNe9puoNUAeNrZRo1Dx1'
    }
  }
}